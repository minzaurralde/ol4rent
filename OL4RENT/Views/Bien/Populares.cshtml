@model IEnumerable<Ol4RentAPI.DTO.BienListadoDTO>

@{
    ViewBag.Title = "Populares";
}
<h2>Bienes Populares</h2>
<ul>
@foreach (Ol4RentAPI.DTO.BienListadoDTO item in Model) {
    <li>
        <div>
            @Html.ActionLink(item.Titulo, "Details", "Bien", new { id=item.Id }, null) <input type="button" value="Me Gusta"  onclick="javascript: like(@item.Id);" />
        </div>
        <div>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </div>
    </li>
}
</ul>
@section Scripts {
    @Scripts.Render("~/Scripts/jquery-1.7.1.js")
    @Scripts.Render("~/Scripts/meGusta.js")
}
